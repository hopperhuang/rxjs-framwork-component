{
    "env": {
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "plugins": [
      "import",
      "jsx-a11y",
      "react",
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "no-underscore-dangle": 0,
      "react/prop-types": 0,
      "import/no-webpack-loader-syntax": 0,
      "func-names": 0,
    }
}
