{
  "env": {
    "production": {
      "presets":[
        "es2015",
        "react",
        "stage-2",
        "env"
      ],
      "plugins": [
         ["import", { "libraryName": "antd", "style": "css" }]
      ],
    },
    "development": {
      "presets":[
        ["es2015", {"modules": false}],
        "react",
        "stage-2",
        ["env", {"modules": false}]
      ],
      "plugins": [
        "react-hot-loader/babel",
        ["import", { "libraryName": "antd", "style": "css" }]
      ],
    },
    "test": {
      "presets": [
        "es2015",
        "stage-2",
        "react"
      ],
      "plugins": [
        ["import", [{ "libraryName": "antd", "style": true }]],
      ]
    }
  }
}
